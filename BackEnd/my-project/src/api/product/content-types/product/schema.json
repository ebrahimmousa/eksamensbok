{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "productName": {
      "type": "string",
      "required": true
    },
    "productPrice": {
      "required": true,
      "type": "decimal"
    },
    "bookAuthor": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "productImage": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": true,
      "required": true
    },
    "Category": {
      "type": "enumeration",
      "enum": [
        "Books",
        "Audio Books"
      ]
    }
  }
}
